{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "eslint.validate": [
    "vue",
    "javascript",
    "typescript"
  ],
  "volar.takeOverMode.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "cspell": {
    "words": [
      "Autotrading",
      "pinia"
    ]
  }
}